###############################################################################
# AJ STUDIOZ AI Configuration
# Environment variables for the React frontend application
###############################################################################

# OpenAI API key retrieved here: https://platform.openai.com/api-keys
REACT_APP_OPENAI_API_KEY=[YOUR_OPENAI_API_KEY]

# Search Configuration
REACT_APP_TAVILY_API_KEY=[YOUR_TAVILY_API_KEY]  # Get your API key at: https://app.tavily.com/home

###############################################################################
# Optional AI Providers
# Enable alternative AI models by configuring these providers
###############################################################################

# Google Generative AI
REACT_APP_GOOGLE_API_KEY=[YOUR_GOOGLE_GENERATIVE_AI_API_KEY]

# Anthropic (Claude)
REACT_APP_ANTHROPIC_API_KEY=[YOUR_ANTHROPIC_API_KEY]

# Groq
REACT_APP_GROQ_API_KEY=[YOUR_GROQ_API_KEY]

# DeepSeek
REACT_APP_DEEPSEEK_API_KEY=[YOUR_DEEPSEEK_API_KEY]

# xAI (Grok)
REACT_APP_XAI_API_KEY=[YOUR_XAI_API_KEY]

###############################################################################
# Backend Configuration (if using separate backend)
###############################################################################
# REACT_APP_FASTAPI_URL=http://localhost:8080  # Only if using separate backend

###############################################################################
# Chat History and Additional Features
###############################################################################
# Enable sharing of chat conversations
REACT_APP_ENABLE_SHARE=true

# Video search capability
REACT_APP_SERPER_API_KEY=[YOUR_SERPER_API_KEY]

# Advanced search providers
REACT_APP_FIRECRAWL_API_KEY=[YOUR_FIRECRAWL_API_KEY]
REACT_APP_JINA_API_KEY=[YOUR_JINA_API_KEY]

